#ifndef _CACHE_H
#define _CACHE_H

#include<iostream>
#include<string>

struct CacheEntry{
  int set, way;
  string address;
  int freq;
  bool lru;
  bool dirty;
};


class Cache{
  CacheEntry** cache;
  
  Cache(int size, int associativity, int blockSize, int replacementPolicy);
  void read(string address);
  void write(string address);
  
  // do we really need these?
  void get(string address);
  void put(string address);
  
  
};

#endif
